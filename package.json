{
  "name": "agg",
  "version": "0.1.0",
  "scripts": {
    "start": "DEBUG=agg ./bin/www",
    "postinstall": "./node_modules/bower/bin/bower install",
    "build": "browserify browser/main.js -o public/js/bundle.js",
    "watch": "watchify browser/main.js -o public/js/bundle.js --debug --verbose"
  },
  "engines": {
    "node": "0.10.x"
  },
  "main": "app.js",
  "dependencies": {
    "backbone": "^1.1.2",
    "body-parser": "^1.9.2",
    "cookie-parser": "^1.3.3",
    "debug": "^2.1.0",
    "express": "^4.10.1",
    "handlebars": "^3.0.2",
    "hogan-express": "^0.5.2",
    "jquery": "^2.1.3",
    "less-middleware": "^1.0.4",
    "lodash": "^2.4.1",
    "moment": "^2.10.2",
    "morgan": "^1.5.0",
    "newrelic": "^1.14.7",
    "serve-favicon": "^2.1.6"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "gulp": "^3.8.10",
    "gulp-jshint": "^1.9.0",
    "hbsfy": "^2.2.1",
    "jshint-stylish": "^1.0.0",
    "watchify": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamgruber/agg"
  },
  "browser": "browser/main.js",
  "browserify": {
    "transform": [
      [
        "hbsfy",
        {
          "extensions": [
            "mu"
          ]
        }
      ]
    ]
  }
}
